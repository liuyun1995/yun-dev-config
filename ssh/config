Host github.com
	User git
    HostName github.com
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/id_rsa_github

Host gitee.com
	User git
	HostName gitee.com
	PreferredAuthentications publickey
	IdentityFile ~/.ssh/id_rsa_gitee

Host gitlab.ihr360.com
	User git
    HostName gitlab.ihr360.com
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/id_rsa_ihr360
