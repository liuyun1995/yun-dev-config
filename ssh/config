Host github.com
	User git
    HostName github.com
	# ProxyCommand nc -v -x 127.0.0.1:1080 %h %p
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/id_rsa_github

Host gitee.com
	User git
	HostName gitee.com
	PreferredAuthentications publickey
	IdentityFile ~/.ssh/id_rsa_gitee

Host bitbucket.telenav.com
	User git
    HostName bitbucket.telenav.com
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/id_rsa_telenav
