my_dir="$(dirname "$0")"

[ -f $my_dir/ssh_count ] && . $my_dir/ssh_count

function ssh_login(){
	expect -c "
		set timeout 30
		spawn ssh $2@$1
		expect \"*password:\"
		send \"$3\r\"
		expect \"ubuntu@*\"
		send \"sudo su\r\"
		expect \"*password*\"
		send \"$3\r\"
		expect \"root*\"
		send \"alias ku='kubectl'\r\"
		interact
	"
}